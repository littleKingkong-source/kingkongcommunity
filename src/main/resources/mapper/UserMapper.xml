<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.littlekingkong.community.dao.UserMapper">

  <insert id="insertUser" parameterType="com.littlekingkong.community.model.User">
      insert into user(account_id,name,token,gmt_create,gmt_modified,avatar_url) values(#{account_id},#{name},#{token},#{gmt_create},#{gmt_modified},#{avatar_url})
  </insert>

    <select id="findByTonken" resultType="com.littlekingkong.community.model.User">
        select id,account_id,name,token,gmt_create,gmt_modified
        from user
        where token = #{token}
    </select>


</mapper>